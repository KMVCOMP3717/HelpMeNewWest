<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cast_expanded_controller_ad_container_white_stripe_color"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="set3r.kmv.ca.helpmenewwest.Main">


    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:background="@drawable/parkimg"
        android:onClick="onClickPark"
        android:layout_above="@+id/imageButton6"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="26dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:text="PARK"
        android:textColor="@android:color/white"
        android:textSize="45sp"
        android:textStyle="bold"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/police"
        android:onClick="onClickEm"
        android:layout_above="@+id/imageButton4"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="12dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EMERGENCY"
        android:textColor="@android:color/white"
        android:textSize="45sp"
        android:textStyle="bold"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:background="@drawable/community"
        android:onClick="onClickComm"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageButton4"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="32dp"
        android:text="COMMUNITY"
        android:textColor="@android:color/white"
        android:textSize="45sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        app:srcCompat="@drawable/logo"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
